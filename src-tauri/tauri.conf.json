{
  "build": {
    "beforeBuildCommand": "",
    "frontendDist": "../dist",
    "beforeDevCommand": "",
    "devUrl": "https://techllama.xyz"
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "windows": {
      "certificateThumbprint": null,
      "digestAlgorithm": "sha256",
      "timestampUrl": ""
    },
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ],
    "resources": [],
    "externalBin": [],
    "copyright": "",
    "category": "DeveloperTool",
    "shortDescription": "",
    "longDescription": "",
    "macOS": {
      "frameworks": [],
      "minimumSystemVersion": "",
      "exceptionDomain": "",
      "signingIdentity": null,
      "providerShortName": null,
      "entitlements": null
    },
    "linux": {
      "deb": {
        "depends": []
      }
    }
  },
  "plugins": {
    "global-shortcut": {}
  },
  "identifier": "com.techllama.wrapper",
  "app": {
    "windows": [
      {
        "title": "TechLlama",
        "width": 800,
        "height": 600,
        "resizable": true,
        "fullscreen": false
      }
    ],
    "security": {
      "csp": "default-src 'self' https://techllama.xyz; script-src 'self' https://techllama.xyz 'unsafe-inline' 'unsafe-eval'; style-src 'self' https://techllama.xyz 'unsafe-inline'; img-src 'self' https://techllama.xyz data: https:; font-src 'self' https://techllama.xyz data: https:;; connect-src ipc: http://ipc.localhost"
    }
  }
}